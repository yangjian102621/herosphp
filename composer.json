{
    "name": "herosphp/framework",
    "description": "a lightweight PHP development framework - herosphp Framework",
    "type": "framework",
    "keywords": [
        "php framework",
        "herosphp",
        "lightweight"
    ],
    "homepage": "https://github.com/yangjian102621/herosphp",
    "license": "MIT",
    "authors": [
        {
            "name": "blackfox",
            "email": "yangjian102621@gmail.com"
        }
    ],
    "require": {
        "workerman/workerman": "^4.0",
        "nikic/fast-route": "^1.3",
        "herosphp/laravel-orm": "*@dev",
        "herosphp/event": "*@dev",
        "herosphp/crontab": "*@dev"
    },
    "autoload": {
        "files": [],
        "psr-4": {
            "herosphp\\": "./src",
            "app\\": "./app",
            "process\\": "./process"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.4"
    },

    "repositories": [
        {
            "type": "path",
            "url": "./laravel-orm"
        },
        {
            "type": "path",
            "url": "./event"
        },
        {
            "type": "path",
            "url": "./contrab"
        }
    ],
    "minimum-stability": "dev"
}
